{
  "name": "@airporting/ebics-parser",
  "version": "2.0.0",
  "main": "src/index.js",
  "repository": "git@github.com:airporting/ebics-parser.git",
  "author": "mathrobin <mthrobin@gmail.com>",
  "license": "MIT",
  "type": "module",
  "packageManager": "yarn@4.2.2",
  "scripts": {
    "commit": "cz",
    "crazy:up": "git pull && yarn set version latest && yarn up '*' && yarn install && yarn prettier:write && git add package.json yarn.lock && git status",
    "husky": "husky install",
    "lint": "eslint -c eslint.config.js",
    "prepare": "husky install",
    "prettier": "prettier --check ./",
    "prettier:write": "prettier --write ./",
    "publish": "yarn npm publish && git add package.json yarn.lock && git commit -m 'feat(version): bump' && git push && git status",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest) --runInBand --config jest.config.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.24.5",
    "@babel/plugin-syntax-import-assertions": "7.24.1",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@eslint/js": "9.2.0",
    "@jest/globals": "29.7.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.2.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "28.5.0",
    "globals": "15.2.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5"
  },
  "dependencies": {
    "date-fns": "3.6.0",
    "joi": "17.13.1"
  }
}
